{
  "build": {
    "beforeDevCommand": "dotnet run --project ./src-blazor/TauriBlazor.csproj",
    "beforeBuildCommand": "dotnet publish ./src-blazor/TauriBlazor.csproj -c Release  -o ./dist -p:BlazorEnableCompression=false",
    "devPath": "http://localhost:5220",
    "distDir": "../dist/wwwroot",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri-blazor-demo",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.SyminStudio.TauriBlazor",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "tauri-blazor-demo",
        "width": 800
      }
    ]
  }
}
